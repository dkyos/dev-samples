input {
    file {
        path => "/home/dkyun77/dkyos/github_dkyos/dev-samples/ELK/data/seg.csv"
        start_position => "beginning"
        sincedb_path => "/dev/null"
    }
}
filter {
    csv {

        separator => "|"

        columns => [ "Consumer", "B1", "B1_raw", "B2", "B2_raw", "B3", "B3_raw", 
        "L1", "L1_raw", "L2", "L2_raw", "L3", "L3_raw", "L4", "L4_raw", 
        "L5", "L5_raw", "L6", "L6_raw", "L7", "L7_raw", "L8", "L8_raw", 
        "L9", "L9_raw", "x", "y", "x_rank", "y_rank", "segmentation"]
    }
    mutate {convert => ["B1", "integer"] }
    mutate {convert => ["B2", "integer"] }
    mutate {convert => ["B3", "integer"] }
    mutate {convert => ["L1", "integer"] }
    mutate {convert => ["L2", "integer"] }
    mutate {convert => ["L3", "integer"] }
    mutate {convert => ["L4", "integer"] }
    mutate {convert => ["L5", "integer"] }
    mutate {convert => ["L6", "integer"] }
    mutate {convert => ["L7", "integer"] }
    mutate {convert => ["L8", "integer"] }
    mutate {convert => ["L9", "integer"] }
    mutate {convert => ["x", "float"] }
    mutate {convert => ["y", "float"] }
}
output {
    elasticsearch {
        hosts => "localhost"
        index => "seg"
        document_type => "de_seg"
    }
    stdout {}
}
